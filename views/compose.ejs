<%- include('partials/header.ejs') %>
<h1>Compose</h1>
<form action="/compose" method="post" class="form-horizontal">
  <label for="FormControlInput1" class="form-label">Title</label>
  <input type="text" class="form-control" name="title" id="FormControlInput1" />
  <label  class="form-label">Post</label>
  <textarea
    class="form-control"
    id="FormControlTextarea1"
    rows="3"
    name="post"
  ></textarea>
  <button  type="submit" class="btn btn-primary "  href="/" style="margin-top: 1rem;">Publish</button>
</form>
<%- include('partials/footer.ejs') %>
